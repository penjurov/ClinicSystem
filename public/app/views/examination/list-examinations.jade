.container
    .well
        form.form-horizontal(name="listExaminationForm")
            fieldset
                .form-group.pull-right
                    label.col-md-3.control-label Search:
                    .col-md-9
                        input.form-control(type="text" placeholder="Search..." ng-model="search")
        .pull-right(max-size="maxSize" total-items="totalItems" ng-model="currentPage")
        table.table.table-striped.table-hover.project-table
            col(width="30%")
            col(width="35%")
            col(width="35%")
            thead
                tr
                th Patient
                th Information
                th Result
            tbody
                tr(ng-repeat="examination in examinations | filter: search")
                    td {{examination.Patient.firstName + ' ' + examination.Patient.lastName}}
                    td {{examination.Information}}
                    td {{examination.Result}}
                    td
                        a.btn.btn-primary(ng-click="viewExamination(examination._id)") View